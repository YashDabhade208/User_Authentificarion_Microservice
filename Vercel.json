{
    "version": 2,
    "builds": [
      {
        "src": "api/Server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["Routes/**", "Config.js", "Middleware/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }
  